<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.webServer>
		<httpProtocol>
			<!-- Remove X-Powered-By header -->
			<customHeaders>
				<remove name="X-Powered-By" />
			</customHeaders>
		</httpProtocol>
		<security>
			<!-- Remove Server header-->
			<requestFiltering removeServerHeader ="true" />
		</security>
	</system.webServer>
</configuration>